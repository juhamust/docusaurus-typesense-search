{
  "name": "typesense-docs-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:typesense & npm run start:docusaurus",
    "start:typesense": "cd typesense-server && ./run.sh",
    "start:docusaurus": "sudo docusaurus start --host 0.0.0.0 --port 80",
    "build": "npm run build:index && npm run build:docusaurus",
    "build:index": "cd typesense-scraper && ./run.sh",
    "build:docusaurus": "sudo docusaurus build",
    "serve": "npm run serve:docusaurus",
    "serve:docusaurus": "sudo docusaurus serve --host 0.0.0.0 --port 80",
    "docusaurus": "docusaurus",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.6.3",
    "@docusaurus/preset-classic": "3.6.3",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-theme-search-typesense": "^0.23.0-0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.6.3",
    "@docusaurus/tsconfig": "3.6.3",
    "@docusaurus/types": "3.6.3",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}